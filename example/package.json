  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@stoplight/prism-cli": "^4.10.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "openapi-typescript-codegen": "^0.23.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  }
}